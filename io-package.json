	"native": {
		"surplus_id": "",
		"surplus_option": false,
		"surplus_positive": false,
		"wallbox_start": "",
		"wallbox_stop": "",
		"wallbox_power": "",
		"wallbox_connected": "",
		"wallbox_ampere_max": 32,
		"wallbox_ampere_min": 6,
		"car_soc": "",
		"car_update": "",
		"car_capacity": 10,
		"timeout_start": 600,
		"timeout_stop": 600,
		"timeout_protection": 300,
		"timeout_car": 120,
		"timeout_power": 60
	},
	"objects": [],
	"instanceObjects": [
		{
			"_id": "control.forceStart",
			"type": "state",
			"common": {
				"name": "Force start charging",
				"desc": "Starts the charging process without paying attention to surplus",
				"type": "boolean",
				"role": "indicator",
				"read": true,
				"write": true,
				"def": false
			},
			"native": {}
		},
		{
			"_id": "control.mode",
			"type": "state",
			"common": {
				"name": "Mode",
				"desc": "Mode for start/stop/update",
				"type": "number",
				"role": "indicator",
				"read": true,
				"write": true,
				"def": 0,
				"states": {
					"0": "Deactivated",
					"1": "Surplus (with ChargeLimit)",
					"2": "Surplus (ignore ChargeLimit)",
					"3": "Normal (with ChargeLimit)",
					"4": "Normal (ignore ChargeLimit)"
				}
			},
			"native": {}
		},
		{
			"_id": "control.chargeLimit",
			"type": "state",
			"common": {
				"name": "Charge Limit",
				"desc": "Charge Limit in Percent of the car",
				"type": "number",
				"role": "value.battery",
				"read": true,
				"write": true,
				"def": 100,
				"unit": "%"
			},
			"native": {}
		},
		{
			"_id": "control.chargePower",
			"type": "state",
			"common": {
				"name": "Charge Power",
				"desc": "Charge Power to use for the Wallbox",
				"type": "number",
				"role": "value.ampere",
				"read": true,
				"write": true,
				"def": 6,
				"unit": "A"
			},
			"native": {}
		}
	]
}