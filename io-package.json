{
	"common": {
		"name": "chargecontrol",
		"version": "0.0.1",
		"news": {
			"0.0.1": {
				"en": "initial release"
			}
		},
		"titleLang": {
			"en": "ChargeControl"
		},
		"desc": {
			"en": "This adapter allows you in a simple way, to control your Wallbox and the charged vehicle!"
		},
		"authors": [
			"SKB <info@skb-web.de>"
		],
		"keywords": [
			"pv power wallbox vehicle charge"
		],
		"licenseInformation": {
			"type": "free",
			"license": "MIT"
		},
		"platform": "Javascript/Node.js",
		"icon": "chargecontrol.png",
		"enabled": true,
		"extIcon": "https://raw.githubusercontent.com/SKB-CGN/ioBroker.chargecontrol/main/admin/chargecontrol.jpg",
		"readme": "https://github.com/SKB-CGN/ioBroker.chargecontrol/blob/main/README.md",
		"loglevel": "info",
		"tier": 3,
		"mode": "daemon",
		"type": "logic",
		"compact": true,
		"connectionType": "local",
		"dataSource": "poll",
		"adminUI": {
			"config": "json",
			"tab": false
		},
		"supportCustoms": true,
		"eraseOnUpload": true,
		"dependencies": [
			{
				"js-controller": ">=6.0.11"
			}
		],
		"globalDependencies": [
			{
				"admin": ">=5.1.13"
			}
		]
	},
	"native": {
		"surplus_id": "",
		"surplus_option": false,
		"surplus_positive": false,
		"wallbox_start": "",
		"wallbox_stop": "",
		"wallbox_power": "",
		"wallbox_connected": "",
		"wallbox_ampere_max": 32,
		"wallbox_ampere_min": 6,
		"timeout_start": 600,
		"timeout_stop": 600,
		"timeout_protection": 300,
		"timeout_car": 120,
		"timeout_power": 60
	},
	"objects": [],
	"instanceObjects": [
		{
			"_id": "control",
			"type": "channel",
			"common": {
				"name": "Control Settings of ChargeControl"
			},
			"native": {}
		},
		{
			"_id": "info",
			"type": "channel",
			"common": {
				"name": "Info of charging"
			},
			"native": {}
		},
		{
			"_id": "control.mode",
			"type": "state",
			"common": {
				"name": "Mode",
				"desc": "Mode for controlling the start of charge",
				"type": "number",
				"role": "indicator",
				"read": true,
				"write": true,
				"def": 0,
				"states": {
					"0": "Deactivated",
					"1": "Only Surplus",
					"2": "Minimal + Surplus",
					"3": "Fast"
				}
			},
			"native": {}
		},
		{
			"_id": "control.chargeLimit",
			"type": "state",
			"common": {
				"name": "Charge Limit",
				"desc": "Charge Limit in Percent of the car",
				"type": "number",
				"role": "value.battery",
				"read": true,
				"write": true,
				"def": 100,
				"unit": "%"
			},
			"native": {}
		},
		{
			"_id": "control.chargePower",
			"type": "state",
			"common": {
				"name": "Charge Power",
				"desc": "Charge Power to use for the Wallbox",
				"type": "number",
				"role": "value.current",
				"read": true,
				"write": true,
				"def": 6,
				"unit": "A"
			},
			"native": {}
		},
		{
			"_id": "info",
			"type": "channel",
			"common": {
				"name": "Info about charging details"
			},
			"native": {}
		},
		{
			"_id": "info.chargeRemainingDuration",
			"type": "state",
			"common": {
				"name": "Remaining Duration",
				"desc": "Remaining Minutes of current charge process",
				"type": "number",
				"role": "level.timer.sleep",
				"read": true,
				"write": false,
				"def": 0,
				"unit": "Minutes"
			},
			"native": {}
		},
		{
			"_id": "info.vehicleName",
			"type": "state",
			"common": {
				"name": "Name of Vehicle",
				"desc": "Name of the Vehicle currently charging, or empty if no vehicle is connected.",
				"type": "string",
				"role": "text",
				"read": true,
				"write": false
			},
			"native": {}
		},
		{
			"_id": "info.charging",
			"type": "state",
			"common": {
				"name": "Indicate charging",
				"desc": "Indicates if charging is active",
				"type": "boolean",
				"role": "indicator.working",
				"def": false,
				"read": true,
				"write": false
			},
			"native": {}
		},
		{
			"_id": "info.chargingCompleted",
			"type": "state",
			"common": {
				"name": "Indicate charging completed",
				"desc": "Indicates if charging is completed or",
				"type": "boolean",
				"role": "indicator",
				"def": false,
				"read": true,
				"write": false
			},
			"native": {}
		},
		{
			"_id": "info.chargeRemainingPercent",
			"type": "state",
			"common": {
				"name": "Remaining percent of energy",
				"desc": "Remaining percent of energy to charge into the car",
				"type": "number",
				"role": "value.battery",
				"read": true,
				"write": false,
				"def": 0,
				"unit": "%"
			},
			"native": {}
		},
		{
			"_id": "info.chargeRemainingPower",
			"type": "state",
			"common": {
				"name": "Remaining power to charge",
				"desc": "Remaining power to charge into the car",
				"type": "number",
				"role": "value.energy.activ",
				"read": true,
				"write": false,
				"def": 0,
				"unit": "Wh"
			},
			"native": {}
		}
	]
}