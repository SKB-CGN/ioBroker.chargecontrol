		"platform": "Javascript/Node.js",
		"icon": "chargecontrol.png",
		"enabled": true,
		"extIcon": "https://raw.githubusercontent.com/SKB-CGN/ioBroker.chargecontrol/main/admin/chargecontrol.png",
