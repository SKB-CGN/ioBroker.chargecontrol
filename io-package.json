		"connectionType": "local",
		"dataSource": "poll",
		"adminUI": {
			"config": "materialize",
			"tab": false
		},
		"supportCustoms": true,
		"eraseOnUpload": true,
		"dependencies": [
			{
				"js-controller": ">=6.0.11"
