{
   "type": "tabs",
   "tabsStyle": {
      "width": "calc(100% - 100px)"
   },
   "iconPosition": "top",
   "i18n": true,
   "items": {
      "_StandardTab": {
         "type": "panel",
         "label": "main_settings",
         "icon": "data:image/svg+xml;base64,",
         "items": {
            "_header_surplus": {
               "type": "header",
               "size": 2,
               "text": "main_settings",
               "sm": 12
            },
            "resetFinished": {
               "type": "checkbox",
               "label": "main_resetFinished",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 12,
               "xl": 12,
               "newLine": true,
               "default": false
            },
            "_divider0": {
               "newLine": true,
               "type": "divider",
               "color": "primary"
            },
            "_donate": {
               "type": "staticImage",
               "tooltip": "PayPal.Me",
               "noTranslation": true,
               "href": "https://www.paypal.com/donate/?hosted_button_id=XNR6XFB9UZ8UJ",
               "src": "paypal-donate-button.png",
               "xs": 6,
               "sm": 6,
               "md": 4,
               "lg": 2,
               "xl": 1,
               "newLine": true
            },
            "_donateTx": {
               "type": "staticText",
               "text": "donate",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 12,
               "xl": 12
            }
         }
      },
      "_SurplusTab": {
         "type": "panel",
         "label": "title_surplus",
         "icon": "data:image/svg+xml;base64,",
         "items": {
            "_header_surplus": {
               "type": "header",
               "size": 2,
               "text": "subtitle_surplus",
               "sm": 12
            },
            "surplus_id": {
               "type": "objectId",
               "label": "surplus_id",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 6,
               "xl": 6,
               "newLine": true
            },
            "_header_surplus_options": {
               "type": "header",
               "size": 2,
               "text": "additional_settings",
               "sm": 12
            },
            "surplus_option": {
               "type": "checkbox",
               "label": "surplus_option",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 12,
               "xl": 12,
               "newLine": true,
               "default": false
            },
            "surplus_positive": {
               "type": "checkbox",
               "label": "surplus_positive",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 12,
               "xl": 12,
               "newLine": true,
               "default": false
            }
         }
      },
      "_WallboxTab": {
         "type": "panel",
         "label": "title_wallbox",
         "icon": "data:image/svg+xml;base64,",
         "items": {
            "_header_wallbox": {
               "type": "header",
               "size": 2,
               "text": "subtitle_wallbox",
               "sm": 12
            },
            "wallbox_connected": {
               "type": "objectId",
               "label": "wallbox_connected",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 6,
               "xl": 6,
               "newLine": true
            },
            "wallbox_start": {
               "type": "objectId",
               "label": "wallbox_start",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 6,
               "xl": 6,
               "newLine": true
            },
            "wallbox_stop": {
               "type": "objectId",
               "label": "wallbox_stop",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 6,
               "xl": 6,
               "newLine": true
            },
            "wallbox_power": {
               "type": "objectId",
               "label": "wallbox_power",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 6,
               "xl": 6,
               "newLine": true
            },
            "wallbox_ampere_min": {
               "type": "number",
               "label": "wallbox_ampere_min",
               "min": 4,
               "step": 1,
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 6,
               "xl": 6,
               "newLine": true
            },
            "wallbox_ampere_max": {
               "type": "number",
               "label": "wallbox_ampere_max",
               "min": 4,
               "step": 1,
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 6,
               "xl": 6,
               "newLine": true
            }
         }
      },
      "_CarTab": {
         "type": "panel",
         "label": "title_car",
         "icon": "data:image/svg+xml;base64,",
         "items": {
            "_header_car": {
               "type": "header",
               "size": 2,
               "text": "subtitle_car",
               "sm": 12
            },
            "carList": {
               "type": "table",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 12,
               "xl": 12,
               "label": "car_list",
               "items": [
                  {
                     "type": "text",
                     "title": "car_name",
                     "attr": "name",
                     "width": "20%",
                     "default": "New Car"
                  },
                  {
                     "type": "number",
                     "title": "car_capacity",
                     "attr": "capacity",
                     "width": "20%",
                     "default": ""
                  },
                  {
                     "type": "objectId",
                     "title": "car_soc",
                     "attr": "soc",
                     "width": "20%",
                     "default": ""
                  },
                  {
                     "type": "objectId",
                     "title": "car_connected",
                     "attr": "connected",
                     "width": "20%",
                     "default": ""
                  },
                  {
                     "type": "objectId",
                     "title": "car_update",
                     "attr": "update",
                     "width": "20%",
                     "default": ""
                  }
               ]
            }
         }
      },
      "_TimeTab": {
         "type": "panel",
         "label": "title_time",
         "icon": "data:image/svg+xml;base64,",
         "items": {
            "_header_time": {
               "type": "header",
               "size": 2,
               "text": "subtitle_time",
               "sm": 12
            },
            "timeout_start": {
               "type": "number",
               "label": "timeout_start",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 6,
               "xl": 6,
               "newLine": true
            },
            "timeout_stop": {
               "type": "number",
               "label": "timeout_stop",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 6,
               "xl": 6,
               "newLine": true
            },
            "timeout_power": {
               "type": "number",
               "label": "timeout_power",
               "min": 60,
               "step": 1,
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 6,
               "xl": 6,
               "newLine": true
            },
            "timeout_protection": {
               "type": "number",
               "label": "timeout_protection",
               "min": 300,
               "step": 1,
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 6,
               "xl": 6,
               "newLine": true
            },
            "timeout_car": {
               "type": "number",
               "label": "timeout_car",
               "min": 60,
               "step": 1,
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 6,
               "xl": 6,
               "newLine": true
            }
         }
      },
      "_NotificationTab": {
         "type": "panel",
         "label": "title_notification",
         "icon": "data:image/svg+xml;base64,",
         "items": {
            "_header_time": {
               "type": "header",
               "size": 2,
               "text": "subtitle_notification",
               "sm": 12
            },
            "notificationEnable": {
               "type": "checkbox",
               "label": "notification_enable",
               "xs": 12,
               "sm": 12,
               "md": 12,
               "lg": 12,
               "xl": 12,
               "newLine": true,
               "default": false
            },
            "notificationsType": {
               "type": "select",
               "label": "notification_type",
               "xs": 12,
               "sm": 12,
               "md": 8,
               "lg": 4,
               "options": [
                  {
                     "label": "E-Mail",
                     "value": "E-Mail"
                  },
                  {
                     "label": "Discord",
                     "value": "Discord"
                  },
                  {
                     "label": "Gotify",
                     "value": "Gotify"
                  },
                  {
                     "label": "Matrix",
                     "value": "Matrix"
                  },
                  {
                     "label": "Pushover",
                     "value": "Pushover"
                  },
                  {
                     "label": "Signal",
                     "value": "Signal"
                  },
                  {
                     "label": "Telegram",
                     "value": "Telegram"
                  },
                  {
                     "label": "WhatsApp",
                     "value": "WhatsApp"
                  }
               ],
               "newLine": true
            },
            "_divider0": {
               "newLine": true,
               "type": "divider",
               "color": "primary"
            },
            "emailInstance": {
               "hidden": "data.notificationsType !== 'E-Mail'",
               "label": "notification_email_instance",
               "type": "instance",
               "adapter": "email",
               "short": false,
               "newLine": true,
               "xs": 12,
               "sm": 12,
               "md": 6,
               "lg": 4,
               "xl": 4
            },
            "telegramInstance": {
               "hidden": "data.notificationsType !== 'Telegram'",
               "label": "notification_telegram_instance",
               "type": "instance",
               "adapter": "telegram",
               "short": false,
               "newLine": true,
               "xs": 12,
               "sm": 12,
               "md": 6,
               "lg": 4,
               "xl": 4
            },
            "telegramRecipient": {
               "type": "selectSendTo",
               "command": "getTelegramUserConfig",
               "jsonData": "{\"instance\": \"${data.telegramInstance}\"}",
               "manual": false,
               "hidden": "data.notificationsType !== 'Telegram'",
               "disabled": "data.telegramInstance == ''",
               "label": "notification_telegram_recipients",
               "newLine": true,
               "xs": 12,
               "sm": 12,
               "md": 6,
               "lg": 4,
               "xl": 4
            }
         }
      }
   }
}